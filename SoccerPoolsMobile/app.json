{
  "expo": {
    "name": "Soccer Pools",
    "slug": "soccer-pools",
    "scheme": "com.soccer-pools-mobile.soccer-pools-mobile",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/img/pools-logo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6860A1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matipendino2001.soccerpools"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/pools-logo2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ],
      "googleServicesFile": "./google-services.json",
      "package": "com.matipendino2001.soccerpools"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3984939241340358~8898058683",
          "iosAppId": "ca-app-pub-3984939241340358~8898058683"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "soccer-pools-mobile",
          "organization": "mappek"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e0e0f555-793f-4652-8175-5ee350c31af5"
      }
    },
    "owner": "matipendino2001"
  }
}
