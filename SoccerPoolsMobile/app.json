{
  "expo": {
    "name": "Soccer Pools",
    "slug": "soccer-pools",
    "scheme": "com.soccer-pools-mobile.soccer-pools-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/pools-logo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matipendino2001.soccerpools"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/pools-logo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.matipendino2001.soccerpools"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "e0e0f555-793f-4652-8175-5ee350c31af5"
      }
    },
    "owner": "matipendino2001"
  }
}
